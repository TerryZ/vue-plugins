/* icons */
@font-face
  font-family "rg-iconfont"
  /* IE9*/
  src url('data:image/eot;base64,')
  src url('data:application/x-font-woff;charset=utf-8;base64,') format('woff'),
  url('data:image/ttf;base64,') format('truetype'), /* chrome, firefox, opera, Safari, Android, iOS 4.2+*/
  url('data:image/svg;base64,') format('svg') /* iOS 4.1- */

.rg-iconfont
  font-family "rg-iconfont" !important
  font-size 14px
  font-style normal
  -webkit-font-smoothing antialiased
  -moz-osx-font-smoothing grayscale

.rg-icon-done:before
  content "\e72e"
.rg-icon-remove:before
  content "\e74b"
.rg-icon-down:before
  content "\e610"
.rg-icon-right:before
  content "\e612"
.rg-icon-clear:before
  content "\e600"